{
  "manifest_version": 3,
  "name": "Video Speed Controller",
  "version": "1.2",
  "description": "Increase video speed up to 100x",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}